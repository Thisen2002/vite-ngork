{
  "name": "my-react-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview --host 0.0.0.0 --port 3000",
    "host": "powershell -ExecutionPolicy Bypass -File ./quick-host.ps1",
    "host:dev": "powershell -ExecutionPolicy Bypass -File ./build-and-host.ps1 -Action both -Environment development",
    "host:prod": "powershell -ExecutionPolicy Bypass -File ./build-and-host.ps1 -Action both -Environment production",
    "build:prod": "powershell -ExecutionPolicy Bypass -File ./build-and-host.ps1 -Action build -Environment production",
    "start:services": "powershell -ExecutionPolicy Bypass -File ./build-and-host.ps1 -Action host -Environment production",
    "host:single": "powershell -ExecutionPolicy Bypass -File ./single-port-host.ps1",
    "host:single-8080": "powershell -ExecutionPolicy Bypass -File ./single-port-host.ps1 -Port 8080"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.57.4",
    "@tailwindcss/vite": "^4.1.13",
    "@types/leaflet": "^1.9.20",
    "axios": "^1.12.0",
    "express": "^5.1.0",
    "framer-motion": "^12.23.12",
    "http-proxy-middleware": "^3.0.5",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.543.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.8.2",
    "recharts": "^3.2.0",
    "socket.io-client": "^4.8.1",
    "tailwindcss": "^4.1.13"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@types/react-router-dom": "^5.3.3",
    "@types/recharts": "^1.8.29",
    "@vitejs/plugin-react": "^5.0.2",
    "@vitejs/plugin-react-swc": "^4.0.0",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2"
  }
}
